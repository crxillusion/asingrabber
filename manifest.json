{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Asin Grabber",
  "description": "Fetch Amazon Standard Identification Numbers (ASINs).",
  "version": "1.0",
  "browser_action": {
    "default_title": "Asin Grabber",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "<all_urls>"
  ]
}